#: utilities/txt2tags.py:212
#, python-format
msgid "Usage: %s [OPTIONS] [infile.t2t ...]"
msgstr "Використання: %s [OPTIONS] [infile.t2t ...]"

#: utilities/txt2tags.py:214
msgid "  -t, --target=TYPE   set target document type. currently supported:"
msgstr ""
"  -t, --target=TYPE встановити тип цільового документа. наразі підтримується:"

#: utilities/txt2tags.py:217
msgid "  -i, --infile=FILE   set FILE as the input file name ('-' for STDIN)"
msgstr ""
"  -i, --infile=FILE встановлює FILE як ім'я вхідного файлу ('-' для STDIN)"

#: utilities/txt2tags.py:218
msgid "  -o, --outfile=FILE  set FILE as the output file name ('-' for STDOUT)"
msgstr ""
"  -o, --outfile=FILE встановлює FILE як ім'я вихідного файлу ('-' для STDOUT)"

#: utilities/txt2tags.py:219
msgid "  -H, --no-headers    suppress header, title and footer contents"
msgstr "  -H, --no-headers приховують вміст верхнього та нижнього колонтитулів"

#: utilities/txt2tags.py:220
msgid ""
"      --headers       show header, title and footer contents (default ON)"
msgstr ""
"      --headers показувати вміст верхнього та нижнього колонтитулів (типово "
"увімкнено)"

#: utilities/txt2tags.py:221
msgid "      --encoding=ENC  set target file encoding (utf-8, iso-8859-1, etc)"
msgstr ""
"      --encoding=ENC встановити кодування цільового файлу (utf-8, iso-8859-1 "
"тощо)"

#: utilities/txt2tags.py:222
msgid "      --style=FILE    use FILE as the document style (like HTML CSS)"
msgstr ""
"      --style=FILE використовувати FILE як стиль документа (наприклад HTML "
"CSS)"

#: utilities/txt2tags.py:223
msgid ""
"      --css-sugar     insert CSS-friendly tags for HTML and XHTML targets"
msgstr "      --css-sugar вставляє CSS-сумісні теги для HTML та XHTML цілей"

#: utilities/txt2tags.py:224
msgid ""
"      --css-inside    insert CSS file contents inside HTML/XHTML headers"
msgstr "      --css-inside вставити вміст файлу CSS у заголовки HTML/XHTML"

#: utilities/txt2tags.py:225
msgid ""
"      --mask-email    hide email from spam robots. x@y.z turns <x (a) y z>"
msgstr ""
"      --mask-email приховати електронну пошту від спам-роботів. x@y.z "
"перетворюється на <x (a) y z>"

#: utilities/txt2tags.py:226
msgid "      --toc           add TOC (Table of Contents) to target document"
msgstr "      --toc додати TOC (Зміст) до цільового документа"

#: utilities/txt2tags.py:227
msgid "      --toc-only      print document TOC and exit"
msgstr "      --toc-only надрукувати зміст документа та вийти"

#: utilities/txt2tags.py:228
msgid "      --toc-level=N   set maximum TOC level (depth) to N"
msgstr ""
"      --toc-level=N встановити максимальний рівень (глибину) змісту на "
"значення N"

#: utilities/txt2tags.py:229
msgid "  -n, --enum-title    enumerate all titles as 1, 1.1, 1.1.1, etc"
msgstr "  -n, --enum-title нумерувати всі заголовки як 1, 1.1, 1.1.1 тощо"

#: utilities/txt2tags.py:230
msgid "  -C, --config-file=F read config from file F"
msgstr "  -C, --config-file=F читати конфігурацію з файлу F"

#: utilities/txt2tags.py:231
msgid "      --rc            read user config file ~/.txt2tagsrc (default ON)"
msgstr ""
"      --rc читати файл конфігурації користувача ~/.txt2tagsrc (типово "
"увімкнено)"

#: utilities/txt2tags.py:232
msgid "      --gui           invoke Graphical Tk Interface"
msgstr "      --gui           викликати графічний інтерфейс Tk"

#: utilities/txt2tags.py:233
msgid "  -q, --quiet         quiet mode, suppress all output (except errors)"
msgstr ""
"  -q, --quiet         тихий режим, придушення всього виводу (крім помилок)"

#: utilities/txt2tags.py:234
msgid "  -v, --verbose       print informative messages during conversion"
msgstr ""
"  -v, --verbose       виводити інформативні повідомлення під час перетворення"

#: utilities/txt2tags.py:235
msgid "  -h, --help          print this help information and exit"
msgstr "  -h, --help           вивести цю довідкову інформацію і вийти"

#: utilities/txt2tags.py:236
msgid "  -V, --version       print program version and exit"
msgstr "  -V, --version       вивести версію програми і вийти"

#: utilities/txt2tags.py:237
msgid "      --dump-config   print all the config found and exit"
msgstr "      --dump-config   вивести всі знайдені конфігурації і вийти"

#: utilities/txt2tags.py:238
msgid "      --dump-source   print the document source, with includes expanded"
msgstr "      --dump-source   вивести джерело документа з розгорнутим вмістом"

#: utilities/txt2tags.py:240
msgid "Turn OFF options:"
msgstr "Вимкнути параметри:"

#: utilities/txt2tags.py:246
#, python-format
